 ; 示例 - 定义一维数组, 返回改序后的数组 
 ; ***************************************************************************** 
 #include <Array.au3> 
 
 Dim $aArray [ 4 ] = [ 1 , 2 , 3 , 4 ] 
 $aNewArray = _ArrayPermute ( $aArray , " , " ) ;使用默认参数 
 _ArrayDisplay ( $aNewArray , " Array Permuted " ) 

